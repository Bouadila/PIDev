{% block body %}
    <div class="content">
    <div class="container-fluid">
    <div class="row">
    <div class="col-md-12">
    <div class="card">
    <div class="card-header card-header-primary">
        <h1 align="center">Votre cv</h1>
        <h4 align="center"> cv </h4>
    </div>
    <div class="card-body">
    <div class="table-responsive">
    <table class="table" border="3">
        <thead class=" text-primary">
        <tr>
            <th>Id</th>
            <th>titre</th>
            <th>statut</th>
        </tr>
        </thead>
        <tbody>
        {% for demande in demandes %}
            <tr>
                <td>{{ demande.id }}</td>
                <td>{{ demande.TitreDemande }}</td>
                <td>{{ demande.statutCand }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}